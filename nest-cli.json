{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "watchAssets": true,
    "assets": [
      {
        "include": "upscayl/bin/upscayl-bin",
        "outDir": "dist"
      },
      {
        "include": "upscayl/bin/models/*",
        "outDir": "dist"
      }
    ]
  }
}
